[user]
    name  = kunaltyagi
    email = tyagi.kunal@live.com
[credential]
    helper = cache
[alias]
    history = log --pretty=format:'%C(red)%h %C(green)%ad%C(reset) | %s%C(yellow)%d%C(reset) [%C(blue)%an%C(reset)]' --graph --date=short --date-order
    relative = log --pretty=format:'%C(red)%h %C(green)%cr%C(reset) | %s%C(yellow)%d%C(reset) [%C(blue)%an%C(reset)]' --graph --date=relative --date-order
    hi   = log --pretty=format:'%C(red)%h %C(green)%ad%C(reset) | %s%C(yellow)%d%C(reset) [%C(blue)%an%C(reset)]' --graph --date=short --date-order
    re   = log --pretty=format:'%C(red)%h %C(green)%cr%C(reset) | %s%C(yellow)%d%C(reset) [%C(blue)%an%C(reset)]' --graph --date=relative --date-order
    type = cat-file -t
    dump = cat-file -p
    ch   = checkout
    co   = commit
    st   = status
    br   = branch
    pu   = push
    fe   = fetch
    di   = diff -w
    sh   = stash
[core]
    editor = vim
[help]
    autocorrect = 0
[color]
    ui = true
