[user]
    name  = Kunal Tyagi
    email = tyagi.kunal@live.com
[alias]
    history = log --pretty=format:'%C(red)%h %C(green)%ad%C(reset) | %s%C(yellow)%d%C(reset) [%C(blue)%an%C(reset)]' --graph --date=short --date-order
    relative = log --pretty=format:'%C(red)%h %C(green)%cr%C(reset) | %s%C(yellow)%d%C(reset) [%C(blue)%an%C(reset)]' --graph --date=relative --date-order
    hi   = log --pretty=format:'%C(red)%h %C(green)%ad%C(reset) | %s%C(yellow)%d%C(reset) [%C(blue)%an%C(reset)]' --graph --date=short --date-order
    re   = log --pretty=format:'%C(red)%h %C(green)%cr%C(reset) | %s%C(yellow)%d%C(reset) [%C(blue)%an%C(reset)]' --graph --date=relative --date-order
    type = cat-file -t
    dump = cat-file -p
    ch   = checkout
    co   = commit
    st   = status
    br   = branch
    pu   = push
    fe   = fetch
    up   = fetch
    di   = diff -w
    sh   = stash
[apply]
    whitespace = fix
[color]
    ui = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[core]
    editor = vim
    autocrlf = input
    whitespace = fix,space-before-tab,tab-in-indent,trailing-space
    excludesfile = ~/.gitignore
[credential]
    helper = cache
[help]
    autocorrect = 0
[merge]
    log = true
[push]
    default = simple
[rerere]
    enabled = 1
[url "git@github.com:"]
    insteadOf = "gh:"
    pushInsteadOf = "github:"
    pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
    insteadOf = "github:"
[url "git@gist.github.com:"]
    insteadOf = "gst:"
    pushInsteadOf = "gist:"
    pushInsteadOf = "git://gist.github.com/"
[url "git://gist.github.com/"]
    insteadOf = "gist:"
[url "git@heroku.com:"]
    insteadOf = "heroku:"

